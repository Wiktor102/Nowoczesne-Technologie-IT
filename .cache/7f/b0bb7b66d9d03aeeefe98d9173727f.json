  easterEggActive = false;\r\n            changeAmmount = 0;\r\n            $('.theme_change > i').removeClass();\r\n\r\n            if (currMode == \"light\") {\r\n                $('.theme_change > i').removeClass().addClass([\"fas\", \"fa-moon\"]);\r\n            } else {\r\n                $('.theme_change > i').removeClass().addClass([\"fas\", \"fa-sun\"]);\r\n            }\r\n\r\n        }, 2500);\r\n    }\r\n\r\n    if (!resetSet) {\r\n        resetSet = true;\r\n        setTimeout(() => {changeAmmount = 0}, 800);\r\n    }\r\n});\r\n\r\n//© Wiktor Golicz 2021 - WSZELKIE PRAWA ZASTRZEŻONE"},"lineCount":null}},"error":null,"hash":"160e556010d858dd33bd2b12a1023148","cacheData":{"env":{}}}